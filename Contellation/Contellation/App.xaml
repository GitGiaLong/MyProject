             xmlns:local="clr-namespace:Contellation"
             xmlns:ui="clr-namespace:Core.Libraries.WPF.Controls;assembly=Core.Libraries.WPF"
             StartupUri="View/Contellation.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
