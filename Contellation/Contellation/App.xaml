                <!--<ResourceDictionary Source="pack://application:,,,/Contellation;component/Resources/Dictionaries/Styles/StyleResource.xaml" />-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
